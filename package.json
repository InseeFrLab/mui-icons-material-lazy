{
    "name": "onyxia-ui-icons",
    "version": "0.0.0",
    "description": "Collection of Material Design icons for use with onyxia-ui",
    "repository": {
        "type": "git",
        "url": "git://github.com/InseeFrLab/onyxia-ui-icons.git"
    },
    "scripts": {
        "build": "tsc",
        "test": "vitest",
        "_format": "prettier '**/*.{ts,tsx,json,md}'",
        "format": "npm run _format -- --write",
        "format:check": "npm run _format -- --list-different"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": "./dist/index.js",
        "./*": "./dist/*.js"
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "eslint --fix"
        ],
        "*.{ts,tsx,json,md}": [
            "prettier --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged -v"
        }
    },
    "author": "u/garronej",
    "license": "MIT",
    "files": [
        "src/",
        "dist/",
        "!dist/tsconfig.tsbuildinfo"
    ],
    "keywords": [],
    "homepage": "https://github.com/InseeFrLab/onyxia-ui-icons",
    "devDependencies": {
        "@types/node": "^22.9.0",
        "typescript": "^5.4.5",
        "husky": "^4.3.8",
        "lint-staged": "^11.1.1",
        "prettier": "^3.3.2",
        "tsx": "^4.15.5"
    },
    "publishConfig": {
        "access": "public"
    }
}
